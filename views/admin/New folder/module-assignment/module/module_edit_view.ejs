<!DOCTYPE html>
<html lang="en">
    
    <%- include('../../admin-layouts/top-header') %>
    <body class="hold-transition fixed sidebar-mini">
        <!-- Preloader -->
        <div class="preloader"></div>

        <div class="wrapper">
            <%- include('../../admin-layouts/header') %>
            
            <div class="content-wrapper">
				 
            	<div class="right_col" Food="main">
					<div class="row">
                        <div class="col-md-12">
							<div class="clearfix"></div>
                            <div class="panel panel-bd lobidrag">
                                
                                <div class="panel-body">
                                    <fieldset class="the-fieldset">
                                        <legend class="the-legend">Update Module</legend>
                                        <form data-toggle="validator" method="post" action="/admin/mod/module/edit/<%= module.id %>"  style="padding-top: 23px">
                                            
                                            
                                            <div class="form-group">
                                                <label for="inputName" class="control-label">Module Name</label>
                                                <input type="text" class="form-control" id="module_name" name="module_name" value="<%= module.module_name %>" placeholder="Enter Module Name" required="">
                                            </div>
                                            
                                            <div class="help-block"></div>
                                            
                                            <div class="form-group">
                                                <label for="inputName" class="control-label">Module Description</label>
                                                <textarea type="text" class="form-control" id="module_description" name="module_description" placeholder="Enter Module Description" required=""><%= module.module_description %></textarea>
                                            </div>
                                            <div class="help-block"></div>
                                       
                                           
                                            <div class="form-group pull-right">
                                                <button type="submit" class="btn btn-cap m-r-5">Submit</button>
                                                <button type="reset" class="btn btn-warning">Cancel</button>
                                            </div>
                                        </form>
                                    </fieldset>
                                </div>
							    		
                            </div>
                        </div>

                    </div>
				</div>
               
            </div>
				<%- include('../../admin-layouts/footer') %>
        </div>
    </body>


</html>